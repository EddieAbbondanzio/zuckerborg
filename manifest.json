{
    "manifest_version": 2,
    "name": "zuckerborg",
    "description": "Utility to fix any typos of Zuckerberg to Zuckerborg.",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}